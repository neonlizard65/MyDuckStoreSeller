<Window x:Class="MyDuckStoreSeller.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyDuckStoreSeller"
        mc:Ignorable="d"
        Title="Авторизация" Height="550" Width="900"
        MaxHeight="600" MaxWidth="900" MinHeight="550" MinWidth="900"
        WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Window.Icon>
        <BitmapImage UriSource="https://www.myduckstudios.fvds.ru/content/icon/web/favicon-32x32.png"/>
    </Window.Icon>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>

        <Border Grid.Row="0" Background="#e4ae11"/>
        <Border Grid.Row="2" Background="#e4ae11"/>
        <Image Grid.Row="0" Source="https://www.myduckstudios.fvds.ru/content/icon/logo_high_res.png" PreviewMouseDown="Image_PreviewMouseDown" Cursor="Hand"/>
        <Frame Name="LoginFrame" NavigationUIVisibility="Hidden" Grid.Row="1"></Frame>
    </Grid>
</Window>
